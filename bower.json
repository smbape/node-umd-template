{
  "name": "umd-template",
  "description": "UMD template projects",
  "version": "0.0.0",
  "main": "**",
  "umd": true,
  "ignore": [
    "**/.*",
    "**/*.log",
    "**/*.sublime-project",
    "**/*.sublime-workspace",
    "bower_components",
    "node_modules",
    "public",
    "work",
    "component.json",
    "package.json"
  ],
  "licenses": "MIT",
  "dependencies": {
    "backbone" : "git+https://github.com/jashkenas/backbone#^1.4.0",
    "bootstrap" : "git+https://github.com/twbs/bootstrap#^4.0.0",
    "i18next" : "git+https://github.com/i18next/i18next#^15.0.4",
    "jquery" : "git+https://github.com/jquery/jquery-dist#^3.3.1",
    "jquery-mockjax" : "git+https://github.com/jakerella/jquery-mockjax#^2.2.1",
    "lodash" : "git+https://github.com/lodash/lodash#^4.17.11",
    "material-design-lite" : "git+https://github.com/google/material-design-lite#^1.3.0",
    "moment" : "git+https://github.com/moment/moment#^2.24.0",
    "popper.js" : "git+https://github.com/FezVrasta/popper.js#^1.14.7",
    "qs" : "git+https://github.com/hapijs/qs#^6.6.0",
    "react": "https://unpkg.com/react@16/umd/react.production.min.js",
    "react-dom": "https://unpkg.com/react-dom@16/umd/react-dom.production.min.js",
    "umd-core" : "git+https://github.com/smbape/node-umd-core.git#^3.0.1"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "jquery": "*",
        "lodash": "*"
      }
    },
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js",
        "dist/fonts/**"
      ],
      "dependencies": {
        "jquery": "*",
        "popper.js": "*"
      },
      "exports": "jQuery.fn.emulateTransitionEnd"
    },
    "i18next": {
      "main": "i18next.js"
    },
    "lodash": {
      "main": "dist/lodash.js"
    },
    "material-design-lite": {
      "main": [
        "material.js"
      ]
    },
    "react-dom": {
      "dependencies": {
        "react": "*"
      }
    }
  }
}
