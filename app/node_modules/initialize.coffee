deps = [
    {common: '!preactCompat'}
]

# 'factory' function will be called when dependencies are resolved
# arguments are the exposed element of the dependencies
factory = (preactCompat)->
    if preactCompat
        window.React = preactCompat
        window.ReactDOM = preactCompat
        # preact.options.syncComponentUpdates = true

    require [
        # where to get the Application class
        './configs/Application'
    ], (Application)->
        app = new Application()
        app.start appConfig
        return

    return
