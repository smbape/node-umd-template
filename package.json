{
  "name": "umd-template",
  "description": "UMD template projects",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "bower install && bower install angular-mocks angular-loader && npm run update-webdriver",
    "test": "npm run test-unit -- --single-run && npm run test-e2e",
    "test-unit": "karma start test/karma.conf.js",
    "test-e2e": "protractor test/protractor-conf.js",
    "test-server": "_mocha --full-trace --compilers coffee:coffee-script/register --require test/server/common.coffee test/server",
    "update-webdriver": "webdriver-manager update"
  },
  "dependencies": {
    "express": "^4.14.0",
    "lodash": "^4.13.1",
    "log4js": "^0.6.37"
  },
  "main": "build.js",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "coffee-script-brunch": "^2.0.0",
    "htmltojsx": "^0.2.5",
    "i18next": "^3.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-requirejs": "^1.0.0",
    "mocha": "^2.5.3",
    "protractor": "^4.0.4",
    "requirejs": "^2.2.0",
    "sass-brunch": "^2.6.3",
    "umd-builder": "^2.5.1",
    "yargs": "^4.8.0"
  }
}
