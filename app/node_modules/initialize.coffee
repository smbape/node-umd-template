deps = [
    {brunch: '!vrdom'}
]

# 'factory' function will be called when dependencies are resolved
# arguments are the exposed element of the dependencies
factory = (vrdom)->
    if vrdom
        window.React = vrdom
        window.ReactDOM = vrdom

    require [
        # where to get the Application class
        './configs/Application'
    ], (Application)->
        app = new Application()
        app.start appConfig
        return

    return
