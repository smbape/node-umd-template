{
  "name": "umd-template",
  "description": "UMD template projects",
  "version": "0.0.0",
  "main": "**",
  "umd": true,
  "ignore": [
    "**/.*",
    "component.json",
    "package.json",
    "node_modules"
  ],
  "licenses": "MIT",
  "dependencies": {
    "backbone": "^1.2.3",
    "bootstrap": "^3.3.6",
    "i18next": "^10.0.7",
    "jquery": "^3.2.1",
    "jquery-mockjax": "^2.1.0",
    "lodash": "^4.17.4",
    "material-design-lite": "^1.1.1",
    "moment": "^2.19.1",
    "qs": "git+https://github.com/hapijs/qs#^6.5.1",
    "react": "^15.3.2",
    "umd-core": "^2.0.0"
  },
  "overrides": {
    "backbone": {
      "dependencies": {
        "jquery": "*",
        "lodash": "*"
      }
    },
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js",
        "dist/fonts/**"
      ],
      "dependencies": {
        "jquery": "*"
      },
      "exports": "jQuery.fn.emulateTransitionEnd"
    },
    "lodash": {
      "main": "dist/lodash.js"
    },
    "material-design-lite": {
      "main": [
        "material.js"
      ]
    },
    "react": {
      "main": [
        "react.min.js"
      ],
      "map": {
        "react-dom.min.js": "react-dom"
      }
    }
  },
  "resolutions": {
    "jquery": "^3.2.1",
    "i18next": "^10.0.7"
  }
}